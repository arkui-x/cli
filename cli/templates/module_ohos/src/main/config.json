{
  "app": {
    "bundleName": "bundleName",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "packageName",
    "name": ".MyApplication",
    "deviceType": [
      "default"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "moduleName",
      "moduleType": "feature",
      "installationFree": false
    },
    "abilities": [
      {
        "visible": true,
        "name": ".MainAbility",
        "srcLanguage": "ets",
        "srcPath": "MainAbility",
        "icon": "$media:icon",
        "description": "$string:MainAbility_desc",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "js": [
      {
        "mode": {
          "syntax": "ets",
          "type": "pageAbility"
        },
        "pages": [
          "pages/index/index"
        ],
        "name": ".MainAbility",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ]
  }
}